{
    "@context": [
        "https://www.w3.org/2018/credentials/v1",
        {
            "@version": 1.1,
            "@protected": true,
            "type": "@type",
            "schema": "https://schema.org/",
            "certificateId": "schema:identifier",
            "taxpayerPIN": "schema:identifier",
            "taxpayerName": "schema:name",
            "businessAddress": "schema:PostalAddress",
            "validityPeriod": {
                "@type": "schema:DateTime",
                "startDate": "schema:startDate",
                "endDate": "schema:endDate"
            },
            "issuingAuthority": {
                "@type": "schema:Organization",
                "name": "schema:name",
                "url": "schema:url"
            },
            "legalStatus": "schema:Text"
        }
    ]
}
