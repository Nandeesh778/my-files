{
  "@context": {
    "@version": 1.1,
    "@protected": true,

    "type": "@type",
    "schema": "https://schema.org/",

    "StudentCredential": {
      "@id": "https://manjunathbhagwat12.github.io/my-files/courses.json#StudentCredential"
    },

    "uuid": "schema:identifier",
    "email": "schema:email",
    "cFullname": "schema:name",
    "dateRegistered": "schema:dateCreated",

    "courses": {
      "@id": "schema:hasCourse",
      "@container": "@list"
    },

    "learningPartner": "schema:provider",
    "courseName": "schema:name",
    "courseHours": "schema:timeRequired",
    "skillLevel": "schema:educationalLevel",
    "courseProgress": "schema:percentComplete",
    "courseGrade": "schema:ratingValue",
    "status": "schema:courseMode",
    "completionDate": "schema:dateCompleted",
    "certificateExpirationDate": "schema:validThrough",

   "credentialStatus": {
      "@id": "vc:credentialStatus",
      "@type": "@json"
    },
    "statusPurpose": "statusList2021:statusPurpose",
    "statusListIndex": "statusList2021:statusListIndex",
    "statusListCredential": "statusList2021:statusListCredential"
  }
}
